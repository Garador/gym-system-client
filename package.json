{
  "name": "gym-system-client",
  "description": "This is the angular client of the gym system. It's main language is Spanish.",
  "author": {
    "name": "azolot",
    "email":"azolotdev@gmail.com"
  },
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && cp -f ./package.json ./dist/",
    "electronstart": "ng build && electron --inspect=5858 ./main.js",
    "electron": "electron ./main.js",
    "winbuild": "rm -rf ./flamingo-client-win32-ia32 && electron-packager . --platform=win32 --electron-version=3.0.13",
    "winstart": "wine ./flamingo-client-win32-ia32/flamingo-client.exe",
    "linuxbuild": "electron-packager . --overwrite --asar --platform=linux --arch=ia32 --icon=assets/flamingo.png --prune=true --out=release-builds"
  },
  "main": "main.js",
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@types/jsonwebtoken": "^8.3.0",
    "core-js": "^2.5.4",
    "dotenv": "^6.2.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "js-datepicker": "^4.0.10",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.23.0",
    "numeral": "^2.0.6",
    "rxjs": "^6.3.3",
    "socket.io-client": "^2.2.0",
    "sql.js": "^0.5.0",
    "tslib": "^1.9.0",
    "typeorm": "^0.2.9",
    "uikit": "^3.0.0-rc.25",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/numeral": "0.0.25",
    "@types/socket.io-client": "^1.4.32",
    "@types/uikit": "^2.27.6",
    "codelyzer": "~4.5.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.38.2",
    "electron-packager": "^13.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.1.6"
  }
}
